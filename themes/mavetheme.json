{
	"name": "Mave Theme (Dark)",
	"type": "dark",
	"colors": {
		"editor.background": "#272727",
		"editor.foreground": "#FFF",
		"editor.selectionBackground": "#204182cc",
		"editor.lineHighlightBackground": "#ffffff0b",
		"editor.activeLineNumber.foreground": "#cccccc",
		"editorGutter.background": "#ffffff0b",
		"titleBar.activeBackground": "#ffffff1a",
		"titleBar.inactiveBackground": "#ffffff10",
		"titleBar.activeForeground": "#cccccc",
		"sideBarTitle.foreground": "#FFF",
		"sideBar.border": "#ffffff1a",
		"sideBar.background": "#242424",

		"activityBar.activeBackground": "#EF7D25",
		"activityBarBadge.background": "#EF7D25",
		"activityBarBadge.foreground": "#FFF",

		"statusBar.background": "#EF7D25",

		"button.background": "#EF7D25",
		"button.hoverBackground": "#EFA46F",

		"menu.selectionBackground": "#EF7D25",
		"menu.foreground": "#FFF",
		"menu.selectionForeground": "#FFF",
		"menubar.selectionForeground": "#FFF"
	},
	"tokenColors": [{
			"name": "Text",
			"scope": [
				"text",
				"text.xml",
				"text.html.basic",
				"text.html.markdown",
				"punctuation.accessor",
				"punctuation.separator.comma",
				"punctuation.separator.array",
				"punctuation.terminator.rule",
				"punctuation.separator.key-value",
				"punctuation.separator.delimiter",
				"punctuation.separator.parameter",
				"punctuation.section.property-list.begin.bracket",
				"punctuation.section.property-list.end.bracket",
				"punctuation.section.keyframes.begin.bracket.curly",
				"punctuation.section.keyframes.end.bracket.curly",
				"punctuation.section.function.begin.bracket.round",
				"punctuation.section.function.end.bracket.round",
				"punctuation.section.function",
				"punctuation.definition.entity",
				"punctuation.definition.block",
				"punctuation.definition.pseudo-class.begin.bracket.round",
				"punctuation.definition.pseudo-class.end.bracket.round",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				"support.class.builtin",
				"support.class.promise",
				"support.class.console",
				"meta.brace.square",
				"meta.brace.round",
				"meta.tag.structure.any",
				"meta.tag.metadata.style",
				"meta.tag.metadata.script",
				"meta.structure.array",
				"meta.structure.dictionary"
			],
			"settings": {
				"foreground": "#F0F0F0"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#787878"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.return-type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"meta.preprocessor.string",
				"meta.embedded",
				"variable.parameter.url.css",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"invalid.illegal.bad-ampersand.html",
				"structure.dictionary.property-name.json",
				"constant.character.escape.scss"
			],
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "REGEXP",
			"scope": ["constant.regexp", "string.regexp"],
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "meta.preprocessor.numeric", "keyword.other.unit", "constant.sha.git-rebase"],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "CSS property value",
			"scope": ["meta.property-value.css"],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.css",
				"variable.scss",
				"variable.language",
				"variable.other.js",
				"variable.other.object.js",
				"variable.other.object.property.js",
				"variable.other.readwrite.js",
				"variable.other.readwrite.alias.js",
				"variable.other.constant.js",
				"variable.other.constant.property.js",
				"variable.other.constant.object.js",
				"variable.other.property.js",
				"variable.object.property.js",
				"variable.other.ts",
				"variable.other.object.ts",
				"variable.other.object.property.ts",
				"variable.other.readwrite.ts",
				"variable.other.readwrite.alias.ts",
				"variable.other.constant.ts",
				"variable.other.constant.property.ts",
				"variable.other.constant.object.ts",
				"variable.other.property.ts",
				"variable.object.property.ts",
				"support.variable",
				"support.other.variable",
				"support.type.property-name",
				"support.type.vendored.property-name",
				"meta.definition.variable.name",
				"meta.object-literal.key",
				"variable.other.elixir",
				"variable.other.object.elixir",
				"variable.other.object.property.elixir",
				"variable.other.readwrite.elixir",
				"variable.other.readwrite.alias.elixir",
				"variable.other.readwrite.module.elixir",
				"variable.other.constant.elixir",
				"variable.other.constant.property.elixir",
				"variable.other.constant.object.elixir"
			],
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "keyword.operator.constructor", "keyword.control", "keyword.other"],
			"settings": {
				"foreground": "#EF7D25"
			}
		},
		{
			"name": "Keyword operator",
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#F0F0F0"
			}
		},
		{
			"name": "Storage",
			"scope": ["storage", "storage.modifier", "storage.type"],
			"settings": {
				"foreground": "#EF7D25"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.module",
				"entity.name.type",
				"entity.other.attribute-name.parent-selector-suffix.css punctuation.definition.entity.css",
				"storage.identifier",
				"support.class"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Function name",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Function argument",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#FD971F"
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#EF7D25"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Library constant",
			"scope": ["support.constant", "support.constant.dom"],
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Language Variable",
			"scope": "variable.language",
			"settings": {
				"foreground": "#EF7D25"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#EF7D25",
				"foreground": "#F0F0F0"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#AE81FF",
				"foreground": "#F0F0F0"
			}
		},
		{
			"name": "Diff",
			"scope": ["meta.diff", "meta.diff.header"],
			"settings": {
				"foreground": "#787878"
			}
		},
		{
			"name": "Markup: deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#EF7D25"
			}
		},
		{
			"name": "Markup: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Markup: changed",
